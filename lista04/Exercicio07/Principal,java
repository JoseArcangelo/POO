public class Principal {
  public static void main(String[] args)
  {
    Empresa e1 = new Empresa();
    Funcionario f1 = new Funcionario("Jose", 1000);
    Funcionario f2 = new Funcionario("Arcangelo", 1500);
    e1.contratar_funcionario(f1);
    e1.contratar_funcionario(f2);
    
    e1.calcular_folha();

    e1.demitir_funcionario("Jose");

    e1.calcular_folha();



  }
}
